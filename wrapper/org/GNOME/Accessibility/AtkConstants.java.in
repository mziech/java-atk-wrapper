package org.GNOME.Accessibility;

/**
 * System and package-dependent constants generated on package build.
 */
public class AtkConstants {

    /**
     * Path to this system's xprop binary.
     */
    static final String XPROP = "@XPROP@";

    /**
     * Name of the binary package containing this wrapper, usually java-atk-wrapper.
     */
    public static final String PACKAGE_NAME = "@PACKAGE_NAME@";

    /**
     * Dot-separated version number of the Java ATK wrapper package, e.g. 0.33.3
     */
    public static final String VERSION = "@VERSION@";

}
